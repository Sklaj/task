{
    "rules": {
      "align": [
        1,
        "parameters",
        "statements"
      ],
      "ban": 0,
      "class-name": 1,
      "comment-format": [
        1
      ],
      "curly": 1,
      "eofline": 2,
      "forin": 1,
      "no-invalid-this": 1,
      "no-unused-new": 1,
      "use-isnan": 1,
      "indent": [
        1,
        4
      ],
      "interface-name": 0,
      "jsdoc-format": 1,
      "label-position": 1,
      "max-line-length": [
        0,
        160
      ],
      "member-access": 1,
      "member-ordering": [
        1,
        "static-before-instance"
      ],
      "no-any": 0,
      "no-arg": 1,
      "no-bitwise": 1,
      "no-conditional-assignment": 1,
      "no-consecutive-blank-lines": [
        1,
        3
      ],
      "no-console": [
        1,
        {
          "allow": ["log"]
        }
      ],
      "no-construct": 1,
      "no-parameter-properties": 1,
      "no-debugger": 1,
      "no-duplicate-variable": 1,
      "no-empty": 1,
      "no-eval": 1,
      "no-inferrable-types": [
        1,
        "ignore-params"
      ],
      "no-internal-module": 1,
      "no-require-imports": 0,
      "no-shadowed-variable": 1,
      "no-string-literal": 0,
      "no-switch-case-fall-through": 1,
      "no-trailing-whitespace": 1,
      "no-unused-expression": [
        1,
        "allow-fast-null-checks"
      ],
      "no-use-before-declare": 1,
      "no-var-keyword": 1,
      "no-var-requires": 0,
      "object-literal-sort-keys": 0,
      "linebreak-style": [
        0,
        "LF"
      ],
      "one-line": [
        1,
        "check-open-brace",
        "check-whitespace"
      ],
      "quotemark": [
        1,
        "double",
        "avoid-escape"
      ],
      "radix": 1,
      "semicolon": [
        1,
        "always",
        "ignore-interfaces",
        "ignore-bound-class-methods"
      ],
      "switch-default": 1,
      "trailing-comma": [
        1,
        {
          "multiline": "never",
          "singleline": "never"
        }
      ],
      "triple-equals": [
        1,
        "allow-null-check"
      ],
      "typedef-whitespace": [
        1,
        {
          "call-signature": "nospace",
          "index-signature": "nospace",
          "parameter": "nospace",
          "property-declaration": "nospace",
          "variable-declaration": "nospace"
        },
        {
          "call-signature": "onespace",
          "index-signature": "onespace",
          "parameter": "onespace",
          "property-declaration": "onespace",
          "variable-declaration": "onespace"
        }
      ],
      "variable-name": [
        1,
        "ban-keywords"
      ],
      "whitespace": [
        1,
        "check-branch",
        "check-decl",
        "check-operator",
        "check-separator",
        "check-type"
      ]
    }
  }
